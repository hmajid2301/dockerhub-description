{
    "name": "dockerhub-description-updater",
    "version": "1.0.0post1",
    "description": "Updates the description of a Docker Hub using README.md.",
    "homepage": "https://gitlab.com/hmajid2301/dockerhub-descriptions-updater#readme",
    "author": "Haseeb Majid",
    "license": "MIT",
    "main": "lib/index.js",
    "repository": {
        "type": "git",
        "url": "git+ssh://git@gitlab.com/hmajid2301/dockerhub-descriptions-updater.git"
    },
    "keywords": [
        "docker",
        "script"
    ],
    "bugs": {
        "url": "https://gitlab.com/hmajid2301/dockerhub-descriptions-updater/issues"
    },
    "bin": {
        "dockerhub_description_updater": "./lib/index.js"
    },
    "scripts": {
        "build": "babel --presets es2015 -d lib/ src",
        "start": "babel-node ./src",
        "code-formatter": "prettier src/**/*.js --write",
        "code-formatter-check": "prettier --check src/**/*.js",
        "lint": "eslint src/**/*.js"
    },
    "dependencies": {
        "babel-plugin-transform-runtime": "6.23.0",
        "lodash": "4.17.21",
        "docker-hub-api": "0.8.0",
        "yargs": "15.3.0"
    },
    "devDependencies": {
        "babel-cli": "6.26.0",
        "babel-preset-env": "1.7.0",
        "babel-preset-es2015": "6.24.1",
        "eslint": "6.8.0",
        "eslint-config-airbnb-base": "14.0.0",
        "eslint-config-prettier": "6.9.0",
        "eslint-plugin-import": "2.20.1",
        "eslint-plugin-prettier": "3.1.2",
        "prettier": "1.19.1"
    }
}